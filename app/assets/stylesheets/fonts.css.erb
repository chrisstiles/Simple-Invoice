@font-face {
    font-family: 'Libertad';
    src: url("<%= asset_path('libertad-extrabold.eot') %>");
    src: url("<%= asset_path('libertad-extrabold.eot?#iefix') %>") format("embedded-opentype"),
         url("<%= asset_path('libertad-extrabold.svg#libertadextrabold') %>") format("svg"),
         url("<%= asset_path('libertad-extrabold.woff2') %>") format("woff2"),
         url("l<%= asset_path('ibertad-extrabold.woff') %>") format("woff"),
         url("<%= asset_path('libertad-extrabold.ttf') %>") format("truetype");
    font-weight: 800;
    font-style: normal;

}


