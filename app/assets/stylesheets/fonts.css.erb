/* Webfont: Libertad */@font-face {
    font-family: 'Libertad';
    src: url("<%= asset_path('Libertad.eot') %>"); /* IE9 Compat Modes */
    src: url("<%= asset_path('Libertad.eot?#iefix') %>") format('embedded-opentype'), /* IE6-IE8 */
         url("<%= asset_path('Libertad.woff') %>") format('woff'), /* Modern Browsers */
         url("<%= asset_path('Libertad.ttf') %>") format('truetype'), /* Safari, Android, iOS */
         url("<%= asset_path('Libertad.svg#Libertad') %>") format('svg'); /* Legacy iOS */
    font-style: normal;
    font-weight: 400;
    text-rendering: optimizeLegibility;
}

